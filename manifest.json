{
  "manifest_version": 3,
  "name": "Kabir Doha New Tab",
  "version": "1.1.0",
  "description": "Displays beautiful Kabir dohas (2-line poetry) with meanings on every new tab",
  "permissions": [],
  "chrome_url_overrides": {
    "newtab": "src/newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
